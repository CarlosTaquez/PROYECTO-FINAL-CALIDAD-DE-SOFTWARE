			"name": "GET - Lista de Planetas",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code es 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Respuesta contiene planetas\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.bodies).to.be.an('array');\r",
							"});\r",
							"\r",
							"pm.test(\"Hay exactamente 8 planetas\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.bodies.length).to.equal(8);\r",
							"});\r",
							"\r",
							"pm.test(\"Tiempo de respuesta menor a 1000ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
							"});\r",
							"\r",
							"pm.test(\"Respuesta tiene formato JSON\", function () {\r",
							"    pm.response.to.be.json;\r",
							"});\r",
							""
